{
  "name": "grape-track",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "postbuild": "npm run seed",
    "seed:admin": "node --experimental-specifier-resolution=node ./scripts/seed-admin.mjs",
    "seed:permissions": "node --experimental-specifier-resolution=node ./scripts/seed-permissions.mjs",
    "seed:all": "npm run seed:admin && npm run seed:permissions",
    "start": "next start",
    "lint": "next lint",
    "generate": "drizzle-kit generate --out src/drizzle/migrations --schema src/drizzle/schema.ts --dialect postgresql",
    "push": "drizzle-kit push",
    "generate:push": "npm run generate && npm run push"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.2",
    "dotenv": "^17.2.1",
    "drizzle-kit": "^0.31.4",
    "drizzle-orm": "^0.44.4",
    "google-auth-library": "^10.2.1",
    "lucide-react": "^0.536.0",
    "next": "15.4.3",
    "next-auth": "^4.24.7",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.5.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.3",
    "tailwindcss": "^4",
    "ts-node": "^10",
    "typescript": "^5"
  }
}
